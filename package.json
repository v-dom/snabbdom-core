{
  "name": "snabbdom-core",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/v-dom/snabbdom-core"
  },
  "author": "suxxus",
  "license": "ISC",
  "description": "snabbdom core studies",
  "scripts": {
    "dev": "watch -p 'src/**/*' -c 'npm run test & npm run lint'",
    "lint": "eslint --color  src || true",
    "test": "NODE_PATH=src tape src/tests | tap-colorize",
    "check": "npm outdated --depth=0",
    "update": "updtr"
  },
  "devDependencies": {
    "benv": "3.0.0",
    "eslint": "2.8.0",
    "eslint-cli": "1.1.0",
    "eslint-config-eslint": "3.0.0",
    "sinon": "1.17.5",
    "tap-colorize": "1.2.0",
    "tape": "4.5.1",
    "updtr": "0.1.10",
    "watch-cli": "0.2.1"
  },
  "keywords": [
    "virtual dom",
    "snabbdom",
    "studies"
  ],
  "dependencies": {
    "snabbdom": "0.5.0"
  }
}
